<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="popupVO">
			<!-- 필수값 체크 -->
			<field property="popType" depends="required">
				<arg0 key="popupVO.popType" />
			</field>
			<field property="popNm" depends="required">
				<arg0 key="popupVO.popNm" />
			</field>
			<field property="startDttm" depends="required">
				<arg0 key="popupVO.startDttm" />
			</field>
			<field property="endDttm" depends="required">
				<arg0 key="popupVO.endDttm" />
			</field>
			<field property="siteCd" depends="required">
				<arg0 key="popupVO.siteCd" />
			</field>
		</form>
	</formset>
</form-validation>