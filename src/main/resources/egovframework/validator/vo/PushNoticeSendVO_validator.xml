<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="pushNoticeSendVO">
			<!-- 필수값 체크 -->
			<field property="sendSeq" depends="required">
				<arg0 key="pushNoticeSendVO.sendSeq" />
			</field>
			<field property="noticeNo" depends="required">
				<arg0 key="pushNoticeSendVO.noticeNo" />
			</field>
			<field property="userId" depends="required">
				<arg0 key="pushNoticeSendVO.userId" />
			</field>
			<field property="userNm" depends="required">
				<arg0 key="pushNoticeSendVO.userNm" />
			</field>
		</form>
	</formset>
</form-validation>