<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="schVO">
			<!-- 필수값 체크 -->
			<field property="schCd" depends="required">
				<arg0 key="schVO.schCd" />
			</field>
			<field property="title" depends="required">
				<arg0 key="schVO.title" />
			</field>
			<field property="startDttm" depends="required">
				<arg0 key="schVO.startDttm" />
			</field>
			<field property="endDttm" depends="required">
				<arg0 key="schVO.endDttm" />
			</field>
			<field property="writer" depends="required">
				<arg0 key="schVO.writer" />
			</field>
		</form>
	</formset>
</form-validation>