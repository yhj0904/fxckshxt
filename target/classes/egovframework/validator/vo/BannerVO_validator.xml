<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="bannerVO">
			<!-- 필수값 체크 -->
			<field property="bannerCd" depends="required">
				<arg0 key="bannerVO.bannerCd" />
			</field>
			<field property="bannerNm" depends="required">
				<arg0 key="bannerVO.bannerNm" />
			</field>
			<!-- <field property="bannerCont" depends="required">
				<arg0 key="bannerVO.bannerCont" />
			</field> -->
			<field property="startDttm" depends="required">
				<arg0 key="bannerVO.startDttm" />
			</field>
			<field property="endDttm" depends="required">
				<arg0 key="bannerVO.endDttm" />
			</field>
			<field property="useYn" depends="required">
				<arg0 key="bannerVO.useYn" />
			</field>
		</form>
	</formset>
</form-validation>