<form-validation>
	<formset>
		<form name="contentVO">
			<!-- 필수값 체크 -->
			<field property="contId" depends="required">
				<arg0 key="contentVO.contId" />
			</field>
			<field property="contNm" depends="required">
				<arg0 key="contentVO.contNm" />
			</field>
		</form>
	</formset>
</form-validation>