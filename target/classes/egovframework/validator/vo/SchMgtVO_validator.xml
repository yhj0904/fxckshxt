<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="schMgtVO">
			<!-- 필수값 체크 -->
			<field property="code" depends="required">
				<arg0 key="schMgtVO.code" />
			</field>
			<field property="title" depends="required">
				<arg0 key="schMgtVO.title" />
			</field>
			<field property="skinCd" depends="required">
				<arg0 key="schMgtVO.skinCd" />
			</field>
			<field property="typeCd" depends="required">
				<arg0 key="schMgtVO.typeCd" />
			</field>
			<field property="fileCnt" depends="isNum">
				<arg0 key="schMgtVO.fileCnt" />
			</field>		
		</form>
	</formset>
</form-validation>