<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="siteVO">
			<!-- 필수값 체크 -->
			<field property="siteCd" depends="required">
				<arg0 key="siteVO.siteCd" />
			</field>
			<field property="siteNm" depends="required">
				<arg0 key="siteVO.siteNm" />
			</field>
			<field property="templateCd" depends="required">
				<arg0 key="siteVO.templateCd" />
			</field>
		</form>
	</formset>
</form-validation>