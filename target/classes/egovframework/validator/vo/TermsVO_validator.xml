<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="termsVO">		
			<!-- 필수값 체크 -->
			<field property="termsNm" depends="required">
				<arg0 key="termsVO.termsNm" />
			</field>
			<field property="necessaryYn" depends="required">
				<arg0 key="termsVO.necessaryYn" />
			</field>
			<field property="useYn" depends="required">
				<arg0 key="termsVO.useYn" />
			</field>
		</form>
	</formset>
</form-validation>